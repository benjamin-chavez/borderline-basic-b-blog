<%= simple_form_for(@user) do |f| %>
  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :avatar, as: :file %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>



<%= link_to "Change Password", edit_user_registration_path %>


